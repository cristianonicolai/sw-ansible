{
  "id": "ansible",
  "version": "1.0",
  "expressionLang": "jsonpath",
  "name": "Workflow Ansible example",
  "description": "An example of how to integrate with Ansible",
  "start": "trigger",
  "functions": [
    {
      "name": "printMessage",
      "metadata": {
        "type": "sysout"
      }
    },
    {
      "name": "triggerAnsible",
      "operation": "ansible.swagger.json#jobTemplatesJobTemplatesLaunchCreate"
    }
  ],
  "states": [
    {
      "name": "trigger",
      "type": "operation",
      "actions": [
        {
          "name": "ansibleAction",
          "functionRef": {
            "refName": "triggerAnsible",
            "arguments": {
              "id": "7"
            }
          }
        }
      ],
      "end": {
        "terminate": "true"
      }
    }
  ]
}